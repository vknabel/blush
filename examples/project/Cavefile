import cave
import cave.tasks

@cave.Dependencies()
data Dependencies {
        @cave.Prelude()
        data Prelude {
                @tasks.Use("test")
                test
        }
}

@tasks.Name("generate")
@tasks.Help("")
data GenerateTask {
	@Bool
	@tasks.Flag("dry")
	@tasks.Help("")
	isDryRun

	@String
	@tasks.Arg()
	positional
}
